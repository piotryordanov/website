{
  "version": 2,
  "name": "website",
  "builds": [
    { "src": "./static/*", "use": "@now/static" },
    { "src": "package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/post/(?<name>[^/]+)$", "dest": "/post?name=$name" },
    { "src": "/book/(?<name>[^/]+)$", "dest": "/book?name=$name" }
  ]
}
